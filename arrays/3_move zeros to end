https://leetcode.com/problems/move-zeroes/description/

 int n=nums.size();
        int j=0;
        int i=0;
        while(j<n){
            if(nums[i]==0){
                if(nums[j] != 0){
                    swap(nums[i],nums[j]);
                    i++;
                }
            }
            else{
                i++;
            }
            j++;
        }
    }
